   "source": [
    "def f(x): return x**5-3*x**3-5*x+4\n",
    "\n",
    "def df(x): return 5*x**4-9*x**2-5\n",
    "\n",
    "def newton(f,df,x0,tol=1e-6):\n",
    "    x = x0\n",
    "    i = 0\n",
    "    while abs(f(x))>tol:\n",
    "        i += 1\n",
    "        x = x-f(x)/df(x)\n",
    "    return x, i\n",
    "\n",
    "print(newton(f,df,1))"
   ]
